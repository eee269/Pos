<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 
 <configuration>
 	<typeAliases>
 		<!-- 카테고리 -->
 		<typeAlias alias="category" type="com.Travel.domain.CategoryBean"/>
 		<!-- 상품 -->
 		<typeAlias alias="product" type="com.Travel.domain.ProductBean"/>
 		<!-- 재고 -->
 		<typeAlias alias="stock" type="com.Travel.domain.StockBean"/>
 		 		
 		<!-- 직원 -->
 		<typeAlias alias="staffBean" type="com.Travel.domain.StaffBean"/>
 		<!-- 직급 -->
 		<typeAlias alias="positionBean" type="com.Travel.domain.PositionBean"/>
 		<!-- 근태 -->
 		<typeAlias alias="commuteBean" type="com.Travel.domain.CommuteBean"/>
 		
		<!-- 판매내역 -->
		<typeAlias alias="orderBean" type="com.Travel.domain.OrderBean"/>
		
		<!-- 판매상세내역 -->
		<typeAlias alias="orderDetailBean" type="com.Travel.domain.OrderDetailBean"/>
		<!-- 페이지 -->
		<typeAlias alias="pageBean" type="com.Travel.domain.PageBean"/>
		
 	</typeAliases>
 </configuration> 